created:
    - database/factories/CategoryFactory.php
    - database/factories/ArticleFactory.php
    - database/factories/StoreFactory.php
    - database/factories/ArticleInStoreFactory.php
    - database/factories/RentalFactory.php
    - database/factories/InvoiceFactory.php
    - database/migrations/2025_08_27_183551_create_categories_table.php
    - database/migrations/2025_08_27_183552_create_articles_table.php
    - database/migrations/2025_08_27_183553_create_stores_table.php
    - database/migrations/2025_08_27_183554_create_article__in__stores_table.php
    - database/migrations/2025_08_27_183555_create_rentals_table.php
    - database/migrations/2025_08_27_183556_create_invoices_table.php
    - app/Models/Category.php
    - app/Models/Article.php
    - app/Models/Store.php
    - app/Models/Article_In_Store.php
    - app/Models/Rental.php
    - app/Models/Invoice.php
models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    Article: { title: 'string:400 unique', description: longtext, price: 'decimal:10,2', category_id: 'foreign:Category', relationships: 'belongsTo:Category belongsToMany:Store through:Article_In_Store hasMany:Rental' }
    Category: { name: 'string:100 unique', relationships: 'hasMany:Article' }
    Store: { name: 'string:100 unique', address: 'string:255', phone: 'string:20', relationships: 'belongsToMany:Article through:Article_In_Store' }
    Article_In_Store: { article_id: foreign, store_id: foreign, stock: 'integer unsigned', relationships: 'belongsTo:Article belongsToMany:Store hasMany:Rental' }
    Rental: { article_in_store_id: 'foreign:Article_In_Store', user_id: foreign, rented_at: timestamp, returned_at: 'timestamp nullable', notes: 'longtext nullable', relationships: 'belongsTo:Article belongsToMany:Store belongsTo:User hasOne:Invoice' }
    Invoice: { total_amount: 'decimal:10,2', rental_id: foreign, issued_at: timestamp, payment_status: 'enum:pending,paid,cancelled', payment_method: 'enum:credit_card,paypal,cash', relationships: 'belongsTo:Rental' }
